{
  "name": "finance-app",
  "version": "1.3.4",
  "repository": "https://github.com/reynoldputra/finance",
  "description": "Finance app",
  "main": "dist/server/main.js",
  "scripts": {
    "build": "vite build && prisma generate && tsc --build && npm run tsc:path-resolve && node copy-files.js && node install-engines-on-mac.js",
    "tsc:build-watch": "npx tsc-watch -b -w --preserveWatchOutput --pretty --onCompilationComplete \"npm run tsc:path-resolve\" ./src/server/tsconfig.json",
    "tsc:path-resolve": "npx tscpaths -p ./src/server/tsconfig.json -s ./src/server -o ./dist/server",
    "dev": "concurrently \"npm run tsc:build-watch\" \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "start": "npm run build && npm run dev",
    "pack": "npm run build && electron-builder --dir",
    "test": "cross-env TESTING=true NODE_ENV=development npx jest ./src/server/__tests__/",
    "studio": "npx prisma studio",
    "generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "npx ts-node ./prisma/seed/seed.ts",
    "db:test": "npx ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' ./prisma/test/test.ts",
    "migrate:dev": "npx prisma migrate dev",
    "dist": "npm run build && electron-builder",
    "publish": "npm run build && dotenv -- electron-builder -p always"
  },
  "author": "Reynold Inu",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@kayahr/jest-electron-runner": "^29.7.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-jest": "^29.6.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.4.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.1",
    "fs-extra": "^11.1.0",
    "jest": "^29.6.2",
    "postcss-loader": "^7.3.3",
    "replace-in-file": "^6.3.5",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
