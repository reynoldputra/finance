{
  "name": "electron-prisma-trpc-example",
  "version": "1.3.0",
  "repository": "https://github.com/awohletz/electron-prisma-trpc-example-releases",
  "description": "An example repo showing how to use Electron with tRPC and Prisma",
  "main": "dist/server/main.js",
  "scripts": {
    "build": "vite build && prisma generate && tsc --build && node copy-files.js && node install-engines-on-mac.js",
    "dev": "concurrently \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "start": "npm run build && concurrently \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "pack": "npm run build && electron-builder --dir",
    "studio": "npx prisma studio",
    "generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "npx ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' ./prisma/seed/seed.ts",
    "db:test": "npx ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' ./prisma/test/test.ts",
    "migrate:dev": "npx prisma migrate dev",
    "dist": "npm run build && electron-builder",
    "publish": "npm run build && dotenv -- electron-builder -p always"
  },
  "author": "Ayron Wohletz",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.1",
    "fs-extra": "^11.1.0",
    "postcss-loader": "^7.3.3",
    "replace-in-file": "^6.3.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@prisma/client": "^4.7.1",
    "@prisma/engines": "^4.7.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-table": "^8.9.3",
    "@trpc/client": "^10.5.0",
    "@trpc/react-query": "^10.5.0",
    "@trpc/server": "^10.5.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "csv-parser": "^3.0.0",
    "electron-log": "^4.4.8",
    "electron-router-dom": "^1.0.5",
    "lucide-react": "^0.261.0",
    "postcss": "^8.4.27",
    "prisma": "^4.7.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-router-dom": "^6.14.2",
    "superjson": "^1.12.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  }
}
